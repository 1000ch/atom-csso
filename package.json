{
  "name": "csso",
  "version": "4.0.0",
  "private": true,
  "description": "Minify CSS with CSSO",
  "scripts": {
    "test": "apm test",
    "pretest": "eslint ."
  },
  "activationCommands": {
    "atom-text-editor:not([mini])": [
      "csso:minify"
    ]
  },
  "configSchema": {
    "restructure": {
      "title": "Restructure",
      "description": "Restructure CSS on minify",
      "type": "boolean",
      "default": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:1000ch/atom-csso.git"
  },
  "engines": {
    "atom": ">=1.0.0"
  },
  "keywords": [
    "css",
    "csso",
    "minify"
  ],
  "author": {
    "name": "1000ch",
    "email": "shogo.sensui@gmail.com",
    "url": "https://github.com/1000ch"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1000ch/atom-csso/issues"
  },
  "homepage": "https://github.com/1000ch/atom-csso#readme",
  "dependencies": {
    "csso-cli": "^3.0.0",
    "execa": "^4.0.3"
  },
  "devDependencies": {
    "eslint": "^7.12.1"
  }
}
