{
  "name": "csso",
  "version": "2.5.1",
  "private": true,
  "description": "Minify CSS with CSSO",
  "activationCommands": {
    "atom-workspace": [
      "csso:minify"
    ]
  },
  "configSchema": {
    "restructure": {
      "title": "Restructure",
      "description": "Restructure CSS on minify",
      "type": "boolean",
      "default": true
    }
  },
  "repository": "1000ch/atom-csso",
  "engines": {
    "atom": ">=1.0.0"
  },
  "keywords": [
    "css",
    "csso",
    "minify"
  ],
  "author": {
    "name": "1000ch",
    "email": "shogo.sensui@gmail.com",
    "url": "https://github.com/1000ch"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1000ch/atom-csso/issues"
  },
  "homepage": "https://github.com/1000ch/atom-csso#readme",
  "dependencies": {
    "csso": "^2.2.1",
    "execa": "^0.6.0"
  }
}
